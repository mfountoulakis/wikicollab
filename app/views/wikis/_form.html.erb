
<form class="pure-form pure-form-stacked">
    <fieldset>

<%= form_for wiki do |f| %>
  <label><%= f.label :title %></label>
  <%= f.text_field :title %>

  <%= f.label :body %>
  <%= f.text_area :body %>

  


<% if current_user && wiki.user == current_user && current_user.subscription%>

  <%= f.label :collaborators %>

  <%= f.collection_select :collaborator_ids, @users, :id, :name, {}, {multiple: true, class: 'chosen-select'} %>

  <%= f.label :private %>
  <%= f.check_box :private %>

<% end %>

  <%= f.submit "Save" %>
<% end %>
</fieldset>
</div>